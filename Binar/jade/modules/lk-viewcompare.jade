ul.nav.nav-tabs.fav-tab(role="tablist")
	li(role="presentation").active
		a(href="#lk-favs" aria-controls="lk-favs" role="tab" data-toggle="tab") 
			svg(viewBox="0 0 100 100", class="svgico like like-gray")
				use(xlink:href="#like")
			span избранное (2)
	li(role="presentation")
		a(href="#lk-compare" aria-controls="lk-compare" role="tab" data-toggle="tab")
			svg(viewBox="0 0 100 100", class="svgico like comparison")
				use(xlink:href="#comparison")
			span избранное (2)
.tab-content
	.tab-pane.fade.in.active#lk-favs(role="tabpanel")
		.basket-block
			.row
				.col-md-3.vcenter
					a(href="").pic
						img(src="img/camera_in_basket.jpg", alt="")
				.col-md-3.vcenter
					.info
						span.name Olympus OM-D E-M5 mark II kit 12-40mm f/2.8 Black          
				.col-md-3.col-md-offset-2.vcenter
					.price 
						span 69 990 .–
				.col-md-1.vcenter
					a.delete(href="")
						i.fa.fa-trash-o(aria-hidden="true")
