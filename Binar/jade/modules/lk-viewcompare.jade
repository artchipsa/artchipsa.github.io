	.tab-pane.fade#lk-compare(role="tabpanel")
		.compare-view.bottom-add-view
			.pre-compare
				.compare-frame
					ul.slidee
						li
							a(href="#" data-category="0").compare-category
								img(src="img/canon700d.png", alt="")
								span Зеркальные фотоапараты
						li
							a(href="#" data-category="1").compare-category
								img(src="img/compare_camera.png", alt="")
								span со сменной оптикой
				.frame-scrollbar
					.handle
			.compare-block-wrapper
				.compare-block(data-category="0")
					.row.head-row
						.col-md-3.vcenter
							a(href="#").back
								svg(viewBox="0 0 100 100", class="svgico arrow back-arrow")
									use(xlink:href="#ArrowLeft")
								span Назад
						.col-md-8.vcenter
							a(href="").dashed-link Все параметры
